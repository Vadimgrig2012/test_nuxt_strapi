{
  "kind": "collectionType",
  "collectionName": "newses",
  "info": {
    "singularName": "news",
    "pluralName": "newses",
    "displayName": "News"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "title_h1": {
      "type": "string",
      "required": true
    },
    "slug": {
      "type": "uid",
      "targetField": "title_h1",
      "required": true
    },
    "excerpt": {
      "type": "string"
    },
    "content": {
      "type": "blocks",
      "required": true
    },
    "cover": {
      "type": "media",
      "multiple": false,
      "allowedTypes": [
        "images",
        "files"
      ]
    },
    "published": {
      "type": "datetime"
    },
    "seo": {
      "type": "component",
      "component": "seo.shared-seo",
      "repeatable": false
    }
  }
}
